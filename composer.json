{
  "name" : "mt-olympus/hermes",
  "description" : "REST API Client using Hal",
  "require" : {
    "php": "^7.0",
    "ext-json": "*",
    "ext-mbstring": "*",
    "mt-olympus/cerberus": "^1.1.0",
    "nocarrier/hal": "^0.9.12",
    "ramsey/uuid": "^3.4.1",
    "zendframework/zend-diactoros": "^1.6",
    "zendframework/zend-eventmanager": "^3.2",
    "zendframework/zend-http": "^2.7",
    "zendframework/zend-servicemanager": "^3.3"
  },
  "require-dev" : {
    "satooshi/php-coveralls" : "dev-master",
    "squizlabs/php_codesniffer" : "^3.1",
    "phpunit/phpunit" : "^5.6 || ^6.0",
    "zendframework/zend-modulemanager" : "^2.8.2",
    "zendframework/zend-paginator": "^2.8"
  },
  "license" : "MIT",
  "keywords" : [ "api", "rest", "client", "zf2", "hal" ],
  "autoload" : {
    "psr-4" : {
      "Hermes\\" : "src/"
    },
    "classmap" : [ "./" ]
  },
  "suggest" : {
    "los/losbase" : "los/losbase for some abstract classes to speed development ",
    "los/loslog" : "los/loslog for logging"
  },
  "type" : "library",
  "config" : {
    "sort-packages": true,
    "process-timeout" : 5000
  },
  "homepage" : "https://github.com/mt-olympus/Hermes",
  "authors" : [ {
    "name" : "Leandro Silva",
    "email" : "leandro@leandrosilva.info"
  } ]
}
