{
    "name": "mt-olympus/hermes",
    "description": "REST API Client using Hal",
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "ext-mbstring": "*",
        "los/cerberus": "^1.0",
        "nocarrier/hal": "^0.9",
        "ramsey/uuid": "^3.4",
        "laminas/laminas-diactoros": "^2.0",
        "laminas/laminas-eventmanager": "^3.2",
        "laminas/laminas-http": "^2.7",
        "laminas/laminas-servicemanager": "^3.3"
    },
    "license": "MIT",
    "keywords": [
        "api",
        "rest",
        "client",
        "zf2",
        "hal"
    ],
    "autoload": {
        "psr-4": {
            "Hermes\\": "src/"
        },
        "classmap": [
            "./"
        ]
    },
    "type": "library",
    "config": {
        "sort-packages": true,
        "process-timeout": 5000
    },
    "homepage": "https://github.com/mt-olympus/hermes",
    "authors": [
        {
            "name": "Leandro Silva",
            "email": "leandro@leandrosilva.info"
        }
    ]
}
